local function GTUXFZ_fake_script() -- TextButton_7.LocalScript 
	local script = Instance.new('LocalScript', TextButton_7)

	local button = script.Parent -- Assuming the button is a direct child of a UI element like a ScreenGui
	
	-- Function to execute the script
	local function executeLoader()
		-- Set the loader configurations
		getgenv().JH_FAST_LOAD = true
		getgenv().JH_TOGGLE_BUTTON = false
		getgenv().JH_TOGGLE_KEYBIND = "LeftControl"
	
		-- Load Jayden's Hub
		loadstring(game:HttpGet('https://raw.githubusercontent.com/Jxys3rrV/jaydens-hub/main/script'))()
	end
	
	-- Event handler for button click
	button.MouseButton1Click:Connect(function()
		executeLoader()
	end)
	
end
coroutine.wrap(GTUXFZ_fake_script)()
