-- Scripts:

local function GKFICNS_fake_script() -- frame.LocalScript 
	local script = Instance.new('LocalScript', frame)

	-- Drag GUI Script
	-- Works for both desktop and mobile (iOS)
	
	-- Settings
	local dragSpeed = 0.7 -- Adjust drag speed as needed
	
	-- Functions
	local function enableDragging(gui)
		local dragging
		local dragInput
		local dragStart
		local startPos
	
		local function update(input)
			local delta = input.Position - dragStart
			gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		end
	
		gui.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
				dragging = true
				dragStart = input.Position
				startPos = gui.Position
	
				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						dragging = false
					end
				end)
			end
		end)
	
		gui.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
				dragInput = input
			end
		end)
	
		game:GetService("UserInputService").InputChanged:Connect(function(input)
			if input == dragInput and dragging then
				update(input)
			end
		end)
	end
	
	-- Example Usage:
	local gui = script.Parent -- Assuming the GUI is a direct child of the script's parent
	enableDragging(gui)
	
end
coroutine.wrap(GKFICNS_fake_script)()
local function VJIU_fake_script() -- TextButton_2.LocalScript 
	local script = Instance.new('LocalScript', TextButton_2)

	local button = script.Parent -- Assuming the button is a direct child of a UI element like a ScreenGui
	
	-- Function to execute the script
	local function executeScript()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/lerkermer/lua-projects/master/SwagModeV002"))()
	end
	
	-- Event handler for button click
	button.MouseButton1Click:Connect(function()
		executeScript()
	end)
	
end
coroutine.wrap(VJIU_fake_script)()
